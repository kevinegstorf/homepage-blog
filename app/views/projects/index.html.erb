<h1 id="page_title">Stuff i've build</h1>
<div id="post_wrapper" class="skinny-wrapper">
  <% @projects.each do |project| %>
    <div class="post">
      <p class="date"> <%= project.created_at.strftime('%A, %B, %d') %></p>
      <h2><a href="#"><%= link_to project.title, project %></a></h2>
      <hr>
    </div>
  <% end %>
